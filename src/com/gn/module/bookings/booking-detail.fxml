<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<StackPane prefHeight="1120.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gn.controller.BookingDetailController">
   <children>
      <TabPane style="-fx-tab-min-width: 100;">
         <tabs>
            <Tab fx:id="tabBookingDetail" closable="false" text="Booking Details">
               <content>
                  <VBox alignment="CENTER" prefHeight="2000.0" prefWidth="1000.0" spacing="10.0">
                     <children>
                        <VBox alignment="CENTER_LEFT">
                           <children>
                              <TextFlow prefHeight="54.0" prefWidth="952.0">
                                 <children>
                                    <VBox alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="940.0">
                                       <children>
                                          <TextFlow prefHeight="21.0" prefWidth="940.0">
                                             <children>
                                                <VBox prefHeight="35.0" prefWidth="95.0">
                                                   <children>
                                                      <Label prefHeight="35.0" prefWidth="104.0" text="Search:" wrapText="true">
                                                         <styleClass>
                                                            <String fx:value="h3" />
                                                            <String fx:value="title" />
                                                         </styleClass>
                                                      </Label>
                                                   </children>
                                                </VBox>
                                                <TextField fx:id="etSearchBookingDetail" prefHeight="27.0" prefWidth="844.0" styleClass="rectangle">
                                                   <VBox.margin>
                                                      <Insets bottom="12.0" left="10.0" />
                                                   </VBox.margin>
                                                </TextField>
                                             </children>
                                          </TextFlow>
                                       </children>
                                    </VBox>
                                 </children>
                              </TextFlow>
                           </children>
                        </VBox>
                        <VBox prefHeight="1007.0" prefWidth="940.0">
                           <children>
                              <TableView fx:id="tvBookingDetails" prefHeight="965.0" prefWidth="940.0">
                                 <columns>
                                    <TableColumn id="BookingDetailId" fx:id="colBookingDetailId" text="Detail ID" />
                                    <TableColumn fx:id="colIteneraryNo" text="Itinerary No" />
                                    <TableColumn id="TripStart" fx:id="colTripStart" text="Trip Start" />
                                    <TableColumn id="TripEnd" fx:id="colTripEnd" text="Trip End" />
                                    <TableColumn fx:id="colDescription" text="Description" />
                                    <TableColumn id="Destination" fx:id="colDestination" text="Destination" />
                                    <TableColumn id="BasePrice" fx:id="colBasePrice" text="Base Price" />
                                    <TableColumn id="TripStart" fx:id="colAgencyCommission" text="AgencyCommission" />
                                    <TableColumn id="BookingID" fx:id="colBookingIdDetail" text="Booking ID" />
                                    <TableColumn id="TripEnd" fx:id="colRegionId" text="Region ID" />
                                    <TableColumn id="Destination" fx:id="colClassId" text="Class ID" />
                                    <TableColumn id="BasePrice" fx:id="colFeeId" text="Fee ID" />
                                    <TableColumn id="BookingID" fx:id="colProductSupplierId" text="Product Supplier ID" />
                                 </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                           </children>
                        </VBox>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="30.0" right="30.0" top="30.0" />
                     </padding>
                  </VBox>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
   <stylesheets>
      <URL value="@../../theme/css/material-color.css" />
      <URL value="@../../theme/css/skeleton.css" />
      <URL value="@../../theme/css/light.css" />
      <URL value="@../../theme/css/bootstrap.css" />
      <URL value="@../../theme/css/shape.css" />
      <URL value="@../../theme/css/typographic.css" />
      <URL value="@../../theme/css/helpers.css" />
      <URL value="@../../theme/css/master.css" />
   </stylesheets>
</StackPane>
